{
    "name": "dev4traders/mt4-sdk-php",
    "description": "SDK for using the Metatrader4 API in PHP",
    "keywords": [
        "dev4traders",
        "mt4-sdk-php"
    ],
    "homepage": "https://github.com/dev4traders/mt4-sdk-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikhail",
            "email": "mikha.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.9",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "D4T\\MT4Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "D4T\\MT4Sdk\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
